perfil.click()

time.sleep(3)

abrir = driver.find_element_by_xpath('//*[@id="react-root"]/section/nav/div[2]/div/div/div[3]/div/div[5]/div[2]/div[2]/div[2]/a[1]/div')
abrir.click()

time.sleep(2)

seguindo = driver.find_element_by_xpath('//*[@id="react-root"]/section/main/div/header/section/ul/li[3]/a')
seguindo.click()

time.sleep(1)

unfollow = driver.find_element_by_xpath('/html/body/div[5]/div/div/div[2]/ul/div/li[1]/div/div[3]/button')
unfollow.click()

time.sleep(1)

certo = driver.find_element_by_xpath('/html/body/div[6]/div/div/div/div[3]/button[1]')
certo.click()

time.sleep(1)

unfollow = driver.find_element_by_xpath('/html/body/div[5]/div/div/div[2]/ul/div/li[2]/div/div[3]/button')
unfollow.click()

time.sleep(1)

certo = driver.find_element_by_xpath('/html/body/div[6]/div/div/div/div[3]/button[1]')
certo.click()

time.sleep(1)

unfollow = driver.find_element_by_xpath('/html/body/div[5]/div/div/div[2]/ul/div/li[3]/div/div[3]/button')
unfollow.click()

time.sleep(1)

certo = driver.find_element_by_xpath('/html/body/div[6]/div/div/div/div[3]/button[1]')
certo.click()

